.ball {
  height: 30px;
  width: 30px;
  display: table-cell;
  text-align: center;
  color: white;
  vertical-align: middle;
  border-radius: 50%; /* may require vendor prefixes */
  background: blue;
  cursor: pointer;
}

.ul-reset>li {
    margin: 0;
}
li {
    margin: 15px 0;
}
li {
    display: list-item;
    text-align: -webkit-match-parent;
}
.ul-reset {
    margin: 0;
    padding: 0;
    list-style: none;
}

