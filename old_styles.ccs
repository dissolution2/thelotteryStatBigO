.button {
        background-color: hsl(249, 4%, 61%);
        border: 1px;
        display: inline-block;
        color: white;
        height: 50px;
        width: 50px;
        cursor: pointer;
}
button:hover {
    background: #777555;
}
.button1 {border-radius: 50%;}
.button2 {border-radius: 20px;}

.grid-container {
  display: grid;
  grid-template-columns: auto auto;
  //height: 50%;
  width: 50%;
  gap: 3px;
  background-color: #2196F3;
  padding: 5px;
}

.grid-container > div {
  background-color: rgba(255, 255, 255, 0.8);
  text-align: center;
  padding: 10px 10px;
  font-size: 30px;
}

.table {
    width: 100%;
    margin-bottom: 40px;
}
.address, .alert, .box, .calculator, .casinos, .head-box, .map, .result, .sitemap, .table, .table-wrap, table {
    box-shadow: 0 0 40px rgb(26 26 26 / 25%);
    border-radius: 15px;
    overflow: hidden;
}
*, :after, :before {
    box-sizing: border-box;
    -webkit-backface-visibility: hidden;
    backface-visibility: hidden;
}
